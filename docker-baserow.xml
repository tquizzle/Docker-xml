<?xml version="1.0"?>
<Container version="2">
  <Name>baserow</Name>
  <Repository>baserow/baserow</Repository>
  <Registry>https://hub.docker.com/r/baserow/baserow</Registry>
  <Support>https://forums.unraid.net/topic/138859-support-baserow/</Support>
  <Project>https://baserow.io</Project>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Overview>Baserow is an open source no-code database and Airtable alternative. 
  &#xD;
  Create your own database without technical experience. Our user friendly no-code tool gives you the powers of a developer without leaving your browser.
  &#xD;
  Be sure to set the BASEROW_PUBLIC_URL to your public URL if you plan on using this over Cloudflare Tunnels.
  </Overview>
  <Category>Productivity</Category>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/tquizzle/Docker-xml/master/img/baserow.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1683372400</DateInstalled>
  <DonateText>Buy me a coffee</DonateText>
  <DonateLink>https://ko-fi.com/tquinnelly</DonateLink>
  <Requires/>
  <Config Name="Data" Target="/baserow/data" Default="/mnt/user/appdata/baserow" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/baserow</Config>
  <Config Name="URL" Target="BASEROW_PUBLIC_URL" Default="http://url:port" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">http://url:PORT</Config>
  <Config Name="WebUI" Target="80" Default="8080" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">8080</Config>
</Container>
