<?xml version="1.0"?>
<Container version="2">
  <Name>PufferPanel</Name>
  <Repository></Repository>
  <Registry>https://hub.docker.com/r/pufferpanel/pufferpanel</Registry>
  <Support>https://forums.unraid.net/topic/138869-support-pufferpanel/</Support>
  <Project>https://www.pufferpanel.com/</Project>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Overview>PufferPanel is an open source game server management panel designed to be easy to use and easy to install. PufferPanel supports Minecraft, Forge, Spigot, Sponge, Source Dedicated Servers, BungeeCord, PocketMine, Forge, and much more.</Overview>
  <Category>Game Servers</Category>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/tquizzle/Docker-xml/master/img/pufferpanel.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1683372400</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Data" Target="/etc/pufferpanel" Default="/mnt/user/appdata/pufferpanel/data" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/pufferpanel/data</Config>
  <Config Name="Lib" Target="/var/lib/pufferpanel" Default="/mnt/user/appdata/pufferpanel/var/lib" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/pufferpanel/var/lib</Config>
  <Config Name="Docker SOCK" Target="/var/run/docker.sock" Default="/var/run/docker.sock" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/var/run/docker.sock</Config>
  <Config Name="WebUI" Target="80" Default="8080" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">8080</Config>
  <Config Name="" Target="5657" Default="5657" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">5657</Config>
</Container>
